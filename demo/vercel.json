{
  "buildCommand": "cd frontend && npm install && npm run build",
  "outputDirectory": "frontend/dist",
  "framework": null,
  "rewrites": [
    { "source": "/api/categories", "destination": "/api/categories" },
    { "source": "/api/templates", "destination": "/api/templates" },
    { "source": "/api/prompt/generate/minimal", "destination": "/api/generate-minimal" },
    { "source": "/api/prompt/generate/complex", "destination": "/api/generate-complex" },
    { "source": "/api/review", "destination": "/api/review" }
  ]
}
